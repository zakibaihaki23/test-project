    this.area = response.data.data\n          this.area = []\n          let array = response.data.data\n          for (let i = 0; i < array.length; i++) {\n            this.area.push({\n              name: array[i].city_name,\n              value: array[i].id,\n            })\n          }\n        })\n      },\n      selected(event) {\n        this.$emit('selected', event)\n      },\n    },\n  }\n</script>\n"]}]}