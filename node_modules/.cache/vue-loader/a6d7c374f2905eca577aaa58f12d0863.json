peValue\"\n          >\n            <template slot=\"typeValue\" slot-scope=\"typeValue\">\n              {{ typeValue.text }}\n            </template>\n          </v-select>\n          <p>Address*</p>\n          <v-textarea\n            v-model=\"defaultItem.address\"\n            rounded\n            filled\n            label=\"Address *\"\n            solo\n          >\n          </v-textarea>\n        </div>\n      </v-col>\n      <v-col md=\"6\">\n        <div class=\"form-right\">\n          <p>Phone Number*</p>\n          <v-text-field\n            v-model=\"defaultItem.phone_no\"\n            rounded\n            filled\n            label=\"Phone Number *\"\n            solo\n          >\n          </v-text-field>\n          <p>Warehouse*</p>\n          <v-select\n            :items=\"warehouse\"\n            rounded\n            item-text=\"name\"\n            item-value=\"value\"\n            filled\n            label=\"Warehouse *\"\n            solo\n            v-model=\"warehouseList\"\n          >\n          </v-select>\n        </div>\n      </v-col>\n    </v-row>\n    <br />\n    <br /><br />\n    <br /><br />\n    <br /><br />\n    <br /><br />\n    <br />\n    <v-divider></v-divider>\n    <div class=\"btn\" v-for=\"(item, i) in items\" :key=\"i\">\n      <v-btn\n        :to=\"item.path\"\n        color=\"#E6E9ED\"\n        style=\"margin: 10px; color: #768F9C\"\n        class=\"cancel\"\n        >Cancel</v-btn\n      >\n      <v-btn style=\"margin: 10px;\" class=\"save\" @click=\"save\">Save</v-btn>\n    </div>\n  </div>\n</template>\n\n<script>\n  export default {\n    data() {\n      return {\n        defaultItem: {\n          name: '',\n          address: '',\n          phone_no: '',\n        },\n        warehouse: '',\n        warehouseList: '',\n\n        types: '',\n        typeValue: [\n          {\n            text: 'Helper',\n            value: 1,\n          },\n          {\n            text: 'Helper Picker',\n            value: 2,\n          },\n        ],\n        // form: {\n        //   type: 0,\n        //   warehouse: [\n        //     {\n        //       name: 'Jelambar',\n        //     },\n        //     {\n        //       name: 'Cipondoh',\n        //     },\n        //     {\n        //       name: 'Ciracas',\n        //     },\n        //   ],\n        //   name: '',\n        //   phone_no: '',\n        //   address: '',\n        // },\n\n        items: [\n          {\n            name: 'Help',\n            path: '/helper',\n          },\n        ],\n      }\n    },\n    created() {\n      this.renderData()\n    },\n\n    methods: {\n      renderData() {\n        this.$http.get('/v1/warehouse').then((response) => {\n          // this.editedItem.code = response.data.data.code\n          // this.editedItem.phone_no = response.data.data.phone_no\n          // this.editedItem.warehouse_name = response.data.data.warehouse_name\n          // this.editedItem.name = response.data.data.name\n          this.warehouse = []\n          let array = response.data.data\n          for (let i = 0; i < array.length; i++) {\n            this.warehouse.push({\n              name: array[i].warehouse_name,\n              value: array[i].id,\n            })\n          }\n          // this.  editedItem.address = response.data.data.address\n\n          // this.update = true\n        })\n      },\n      save() {\n        this.$http\n          .post('/v1/helper', {\n            warehouse_id: this.warehouseList,\n            name: this.defaultItem.name,\n            phone_no: this.defaultItem.phone_no,\n            address: this.defaultItem.address,\n            Type: this.types,\n          })\n          .then((response) => {\n            this.$router.push('/helper')\n          })\n          .catch((error) => {\n            console.log(error)\n          })\n\n        // console.log(this.warehouseList)\n        // console.log(this.defaultItem.name)\n        // console.log(this.defaultItem.phone_no)\n        // console.log(this.defaultItem.address)\n        // console.log(this.types)\n\n        // .then(function(response) {\n        //   console.log(response)\n        // })\n        // .catch(function(error) {\n        //   console.log(error)\n        // })\n\n        // console.warn(this.defaultItem)\n        // e.preventDefault()\n      },\n    },\n  }\n</script>\n\n<style scoped>\n  .regist {\n    padding-left: 80px;\n    padding-right: 50px;\n  }\n  .form-right {\n    margin-top: 50px;\n    padding-right: 90px;\n  }\n  .name {\n    border-radius: 15px;\n  }\n  .v-btn:not(.v-btn--round).v-size--default {\n    margin-top: 50px;\n    background: #4662d4;\n    color: white;\n    box-sizing: content-box;\n    border-radius: 25px;\n    width: 111px;\n    height: 45px;\n  }\n  .btn {\n    margin-top: 30px;\n    padding-left: 1100px;\n  }\n</style>\n"]}]}