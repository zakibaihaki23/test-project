andler: function(val) {\n          this.areaSelected = null\n        },\n        deep: true,\n      },\n      city: {\n        handler: function(val) {\n          if (val) {\n            this.autoSelectById(val)\n          }\n        },\n        deep: true,\n      },\n    },\n    methods: {\n      renderData(search) {\n        this.$http\n          .get('/v1/city', {\n            // params: {\n            //   conditions: 'city_name.icontains:' + search,\n            // },\n          })\n          .then((response) => {\n            this.area = response.data.data\n            if (this.area === null) {\n              this.area = []\n            }\n          })\n      },\n      selected(event) {\n        this.$emit('selected', event)\n      },\n    },\n  }\n</script>\n"]}]}