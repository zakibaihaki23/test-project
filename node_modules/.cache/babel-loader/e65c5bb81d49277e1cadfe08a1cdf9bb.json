reaId) {\n          areaId = 'city_id.e:' + areaId\n        } else {\n          areaId = ''\n        }\n\n        this.$http\n          .get('/v1/warehouse', {\n            params: {\n              conditions: areaId,\n            },\n          })\n          .then((response) => {\n            this.items = response.data.data\n\n            this.items = []\n            let array = response.data.data\n            if (array != null) {\n              for (let i = 0; i < array.length; i++) {\n                this.items.push({\n                  name: array[i].warehouse_name,\n                  value: array[i].id,\n                })\n              }\n            }\n          })\n      },\n\n      selected(event) {\n        this.$emit('selected', event)\n      },\n    },\n  }\n</script>\n"],"sourceRoot":"src/components"}]}